{
  "name": "webpack-multipage",
  "version": "1.0.0",
  "description": "develop multipage static pages with webpack & gulp",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=dev&& webpack-dev-server --hot --inline",
    "dev:ts": "set NODE_ENV=devts&& webpack-dev-server --hot --inline",
    "build": "set NODE_ENV=build&& gulp clean && webpack --progress --profile --colors --config webpack.config.js && gulp",
    "build:ts": "set NODE_ENV=buildts&& gulp clean && webpack --progress --profile --colors --config webpack.config.js && gulp"
  },
  "author": "rongj <10983304@qq.com>",
  "private": true,
  "devDependencies": {
    "@types/node": "^9.4.0",
    "@types/zepto": "^1.0.29",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cache": "^0.4.6",
    "gulp-clean": "^0.3.2",
    "gulp-smushit": "^1.2.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "ts-loader": "^3.4.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "jquery": "^3.2.1",
    "webpack-zepto": "^0.0.1"
  }
}
